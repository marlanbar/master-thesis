keywords = {Classifier evaluation,Evaluation metrics,ROC analysis},
mendeley-groups = {thesis-reference},
number = {8},
pages = {861--874},
title = {{An introduction to ROC analysis}},
volume = {27},
year = {2006}
}
@article{doi:10.1002/humu.22768,
abstract = {ABSTRACT Prioritizing missense variants for further experimental investigation is a key challenge in current sequencing studies for exploring complex and Mendelian diseases. A large number of in silico tools have been employed for the task of pathogenicity prediction, including PolyPhen-2, SIFT, FatHMM, MutationTaster-2, MutationAssessor, Combined Annotation Dependent Depletion, LRT, phyloP, and GERP++, as well as optimized methods of combining tool scores, such as Condel and Logit. Due to the wealth of these methods, an important practical question to answer is which of these tools generalize best, that is, correctly predict the pathogenic character of new variants. We here demonstrate in a study of 10 tools on five datasets that such a comparative evaluation of these tools is hindered by two types of circularity: they arise due to (1) the same variants or (2) different variants from the same protein occurring both in the datasets used for training and for evaluation of these tools, which may lead to overly optimistic results. We show that comparative evaluations of predictors that do not address these types of circularity may erroneously conclude that circularity confounded tools are most accurate among all tools, and may even outperform optimized combinations of tools.},
author = {Grimm, D. G. and Azencott, C. and Aicheler, F. and Gieraths, U. and MacArthur, D. G. and Samocha, K. E. and Cooper, D. N. and Stenson, P. D. and Daly, M. J. and Smoller, J. W. and Duncan, L. E. and Borgwardt, K. M.},
doi = {10.1002/humu.22768},
journal = {Human Mutation},
keywords = { exome sequencing,pathogenicity prediction tools},
mendeley-groups = {thesis-reference},
number = {5},
pages = {513--523},
title = {{The Evaluation of Tools Used to Predict the Impact of Missense Variants Is Hindered by Two Types of Circularity}},
url = {https://onlinelibrary.wiley.com/doi/abs/10.1002/humu.22768},
volume = {36},
year = {2015}
}
