doi = {10.1093/bioinformatics/btv009},
eprint = {NIHMS150003},
file = {:home/marlan/Downloads/btv009.pdf:pdf},
isbn = {1367-4811 (Electronic)$\backslash$r1367-4803 (Linking)},
issn = {14602059},
journal = {Bioinformatics},
number = {10},
